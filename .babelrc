{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "58",
          "ie": "11"
        }
      }
    ],
    ["@babel/preset-react"]
  ],
  "plugins": [
    [
      "module-resolver", {
        "root": ["./src"],
        "alias": {
          "@Components": "./src/Components",
          "@Context": "./src/Context",
          "@Reducer": "./src/Reducer",
          "@Style": "./src/Style",
          "@Ui": "./src/Ui"
        }
      }
    ]
  ]
}